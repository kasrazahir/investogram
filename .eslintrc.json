{
 "root": true,
 "rules": {
	"no-tabs": "off",
	"no-console": "off",
	"prefer-template": "off",
	"react/jsx-indent": "off",
	"arrow-body-style": "off",
	"no-unused-expressions": "off",
	"indent": ["error","tab", {"SwitchCase": 1}],
	"space-infix-ops": 0
 },
 "env": {
	"es6": true,
	"node": true,
	"browser": true
 },
 "extends": [ "airbnb", "plugin:import/warnings" ],
 "parserOptions": {
	"ecmaVersion": 6,
	"sourceType": "module",
	"ecmaFeatures": {
	 "jsx": true,
	 "experimentalObjectRestSpread": true
	 }
 }
}
